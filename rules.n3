@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix ai: <http://example.org/input#> .
@prefix airo: <https://w3id.org/airo#> .
@prefix vair: <https://w3id.org/vair#> .

{
    ai:System airo:hasDomain ?Domain .
    ?Domain rdf:type vair:CriticalInfrastructure .
    ai:System airo:hasPurpose ?Purpose .
    ?Purpose rdf:type vair:ManagementAndOperationOfSafety .
} => {
    ai:System airo:hasRiskLevel airo:HighRisk .
    ai:System ai:source ai:AIAct-AnnexIII2a .
} .

{
    ai:System airo:hasDomain ?Domain .
    ?Domain rdf:type vair:CriticalInfrastructure .
    ai:System airo:hasPurpose ?Purpose .
    ?Purpose rdf:type vair:GeneralPurpose .
} => {
    ai:System airo:hasRiskLevel airo:PotentiallyHighRisk .
    ai:System ai:source ai:AIAct-AnnexIII2a .
} .