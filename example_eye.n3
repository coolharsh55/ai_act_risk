@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix ai: <http://example.org/input#> .
@prefix airo: <https://w3id.org/airo#> .
@prefix vair: <https://w3id.org/vair#> .

# test at: https://eyereasoner.github.io/eye-js/example/index.html

{
    ai:System airo:hasDomain ?Domain .
    ?Domain rdf:type vair:CriticalInfrastructure .
} => { ai:System airo:hasRiskSource airo:HighRiskDomain } .

{
    ai:System airo:hasDomain ?Domain .
    ?Domain rdf:type vair:GeneralDomain .
} => { ai:System airo:hasRiskSource airo:GeneralDomain } .

{
    ai:System airo:hasPurpose ?Purpose .
    ?Purpose rdf:type vair:AdvertisingVacancies .
} => { ai:System airo:hasRiskSource airo:HighRiskPurpose } .

{
    ai:System airo:hasPurpose ?Purpose .
    ?Purpose rdf:type vair:GeneralPurpose .
} => { ai:System airo:hasRiskSource airo:GeneralPurpose } .

{
    ai:System airo:hasRiskSource airo:HighRiskDomain .
    ai:System airo:hasRiskSource airo:HighRiskPurpose .    
} => {
    ai:System airo:hasRiskLevel airo:HighRisk .
    ai:System airo:source "AI Act Annex III 3c"@en .
} .

{
    ai:System airo:hasRiskSource airo:HighRiskDomain .
    ai:System airo:hasRiskSource airo:GeneralPurpose .    
} => {
    ai:System airo:hasRiskLevel airo:PossibleHighRisk .
    ai:System airo:source "AI Act Annex III 3c"@en .
} .

ai:System airo:hasDomain ai:Domain .
ai:Domain rdf:type vair:CriticalInfrastructure .

ai:System airo:hasPurpose ai:Purpose .
ai:Purpose rdf:type vair:AdvertisingVacancies .
