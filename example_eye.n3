@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix ai: <http://example.org/input#> .
@prefix airo: <https://w3id.org/airo#> .
@prefix vair: <https://w3id.org/vair#> .

ai:System airo:hasDomain vair:Electricity .
ai:System airo:hasPurpose vair:ManageOperations .

vair:Electricity rdfs:subClassOf vair:CriticalInfrastructure .
vair:ManageOperations rdfs:subClassOf vair:Management .

{
ai:System airo:hasDomain ?Domain .
?Domain rdfs:subClassOf vair:CriticalInfrastructure .

ai:System airo:hasPurpose ?Purpose .
?Purpose rdfs:subClassOf vair:Management .

} => {
ai:System airo:hasRiskLevel airo:HighRisk .
}.